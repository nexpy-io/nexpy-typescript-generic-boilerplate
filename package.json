{
  "name": "nexpy-frontend-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "sh ./auto/env.sh && husky install",
    "reinstall": "yarn concurrently \"rm -rf ./node_modules\" \"rm -rf ./yarn.lock ./package-lock.json\" && yarn",
    "commit": "npx git-cz",
    "type:check": "tsc --project tsconfig.json --pretty --noEmit",
    "prettier": "prettier --loglevel warn --write .",
    "prettier:check": "prettier --check .",
    "eslint": "eslint --fix --max-warnings 0 .",
    "eslint:check": "eslint --max-warnings 0 ."
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "commitlint": "^16.1.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-helpers": "^1.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
